# Development Environment (committed to git for easy setup)
# All values are set to "mock" for development without API keys

# API Gateway
VITE_API_GATEWAY_URL=http://localhost:3001

# OpenAI API Key (mock for development)
OPENAI_API_KEY=mock

# Amadeus API (mock for development)
AMADEUS_API_KEY=mock
AMADEUS_API_SECRET=mock

# Mock Mode (enabled by default for development)
USE_MOCK_RESPONSES=true

# Service URLs (for Docker internal networking)
TYPESCRIPT_AGENTS_URL=http://typescript-agents:3002
PYTHON_AGENTS_URL=http://python-agents:8000
