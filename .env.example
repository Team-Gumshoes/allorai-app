# Port number
PORT=PORT_NUMBER_GOES_HERE

# Google Gemini API Key
GOOGLE_API_KEY=API_KEY_GOES_HERE

# OpenAI API Key
OPENAI_API_KEY=API_KEY_GOES_HERE

# Amadeus Travel API Credentials (OAuth2 - token auto-refreshes)
AMADEUS_CLIENT_ID=CLIENT_ID_GOES_HERE
AMADEUS_CLIENT_SECRET=CLIENT_SECRET_GOES_HERE
AMADEUS_GRANT_TYPE=client_credentials

# Toggle flight data source: "true" for Amadeus API, "false" for LLM-generated data
USE_FLIGHT_API=false

# Google Places API Credentials
GOOGLE_PLACES_API_KEY=API_KEY_GOES_HERE

# Toggle places data source: "true" for Google Places API, "false" for LLM-generated data
USE_PLACES_API=false

# Toggle LLM summary generation: "true" to generate summaries, "false" to skip (returns empty string)
GENERATE_SUMMARIES=false

# Model tiers — set company + model name per complexity tier
# Accepted MODEL_COMPANY values: OpenAI | GoogleGemini | Ollama
# fast     → classifyIntent (routing / classification)
# standard → arithmeticNode, summarizeFlights (moderate reasoning)
# smart    → all travel-planning nodes + generator (full generation)
FAST_MODEL_COMPANY=Ollama
FAST_MODEL_NAME=qwen2.5:3b
STANDARD_MODEL_COMPANY=OpenAI
STANDARD_MODEL_NAME=gpt-4o-mini
SMART_MODEL_COMPANY=OpenAI
SMART_MODEL_NAME=gpt-5-nano